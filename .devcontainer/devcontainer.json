{
  "name": "springboot-webflux-grafana",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {},
  "runArgs": [
    "-v",
    "/var/run/docker.sock:/var/run/docker.sock"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "redhat.vscode-yaml",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "forwardPorts": [8080, 9090, 3000],
  "postCreateCommand": "chmod +x mvnw",
  "remoteUser": "root"
}
